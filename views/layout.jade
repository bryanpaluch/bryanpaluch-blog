!!!
html
  head
    title= 'BryanPaluch.com'
    meta(charset='utf-8')
    link(rel='stylesheet', href='/css/bootstrap.css')
    link(rel='stylesheet', href='/css/app.css')
    link(rel='stylesheet', href='http://yandex.st/highlightjs/7.3/styles/default.min.css')
    script(src='http://yandex.st/highlightjs/7.3/highlight.min.js')
    script(type='text/javascript')
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-35470244-2']);
      _gaq.push(['_trackPageview']);
      (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    script(type='text/javascript')
      hljs.initHighlightingOnLoad();
  body
    .navbar.navbar-static-top
      .navbar-inner
        .container
          a.brand(href='/') Bryan Paluch
    .wrap
      .container-fluid
        .row-fluid
          .span2
            .well.sidebar-nav
              include includes/ads
          .span6
            block content
          .span2
            .row
              .well.sidebar-nav
                include includes/about
            .row
              .well.sidebar-nav
                ul.nav.nav-list
                  li.nav-header categories
                  - each cat in categoryList
                    li
                      a(href=categoryUrl(cat)) #{cat}
                  li.nav-header tags
                  - each tag in tagList
                    li
                      a(href=tagUrl(tag)) #{tag}
